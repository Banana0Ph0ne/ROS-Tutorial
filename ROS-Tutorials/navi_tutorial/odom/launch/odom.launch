<launch>
    <!-- <arg name="robot_ns" default="navi"/> -->
    <!--set the robot namespace-->

    <!--[*kf] can be either "ekf" (Extended Kalman Filter) or "ukf" (Unscented Kalman Filter)-->
    <node pkg="robot_localization" type="ekf_localization_node" name="ekf_localization"  clear_params="true"  output="screen" >
        <!--Outputs odom filtered topic^^^-->
        <remap from="odometry/filtered" to="odometry/local"/>
        <rosparam command="load" file="$(find odom)/config/odom.yaml"/>
        <!--Loads config file ^^^ subst value needed for args to work-->
    </node>
</launch>